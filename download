  <script>
  function getDownload(){
    var file_path = 'lib/main.html';
    var a = document.createElement('A');
    a.href = file_path;
    a.download = file_path.substr(file_path.lastIndexOf('/') + 1);
    document.body.appendChild(a);
    a.click();
    document.body.removeChild(a);
  }



  </script>
  
  
